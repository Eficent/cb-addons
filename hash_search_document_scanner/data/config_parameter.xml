<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="hash_search_document_scanner_path"
                model="ir.config_parameter" forcecreate="True">
            <field name="key">hash_search_document_scanner.path</field>
            <field name="value">/opt/qr_data</field>
        </record>
        <record id="hash_search_document_scanner_ok_path"
                model="ir.config_parameter">
            <field name="key">hash_search_document_scanner.ok_path</field>
            <field name="value">/opt/ok_qr_data</field>
        </record>
        <record id="hash_search_document_scanner_failure_path"
                model="ir.config_parameter">
            <field name="key">hash_search_document_scanner.failure_path</field>
            <field name="value">/opt/ko_qr_data</field>
        </record>
    </data>
</odoo>
