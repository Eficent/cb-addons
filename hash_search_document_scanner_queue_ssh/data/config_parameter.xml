<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="hash_search_document_scanner_host"
                model="ir.config_parameter" forcecreate="True">
            <field name="key">hash_search_document_scanner_queue_ssh.host</field>
            <field name="value">localhost</field>
        </record>
        <record id="hash_search_document_scanner_port"
                model="ir.config_parameter" forcecreate="True">
            <field name="key">hash_search_document_scanner_queue_ssh.port</field>
            <field name="value">22</field>
        </record>
        <record id="hash_search_document_scanner_user"
                model="ir.config_parameter" forcecreate="True">
            <field name="key">hash_search_document_scanner_queue_ssh.user</field>
            <field name="value">odoo</field>
        </record>
        <record id="hash_search_document_scanner_password"
                model="ir.config_parameter" forcecreate="True">
            <field name="key">hash_search_document_scanner_queue_ssh.password</field>
            <field name="value">odoo</field>
        </record>
        <record id="hash_search_document_scanner_ssh_path"
                model="ir.config_parameter">
            <field name="key">hash_search_document_scanner_queue_ssh.ssh_path</field>
            <field name="value"></field>
        </record>
    </data>
</odoo>
